GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
GET     /index                           controllers.HomeController.index
GET     /login                      controllers.LoginController.login
POST    /login                      controllers.LoginController.loginSubmit
POST    /logout                     controllers.logoutController.logout
GET     /signUp                     controllers.registerController.register
POST    /signUp                     controllers.registerController.registerSubmit
GET     /destination                controllers.destinationController.Destination
POST    /destination                controllers.destinationController.destSubmit
GET     /createFromJson             controllers.ApplicationController.createFromJson
GET     /findByName                 controllers.ApplicationController.findByName
GET     /create                     controllers.ApplicationController.create